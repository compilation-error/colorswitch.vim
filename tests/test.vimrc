
set nocompatible
syntax on
filetype plugin indent on

set runtimepath+=~/Workspace/colorswitch.vim

let g:colorswitch_dark_theme='gruvbox'
let g:colorswitch_dark_airline='base16'
let g:colorswitch_light_theme='PaperColor'
